            },
            {
              "id": "gitago.twitchextras.act.poll.choice4",
              "type": "text",
              "label": "Poll Choice 4",
              "default": ""
            },
            {
              "id": "gitago.twitchextras.act.poll.choice5",
              "type": "text",
              "label": "Poll Choice 5",
              "default": ""
