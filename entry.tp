{
  "sdk": 3,
  "version": 100,
  "name": "Gitago Twitch Extras",
  "id": "gitago.twitchextras",
  "configuration": {
    "colorDark": "#25274c",
    "colorLight": "#707ab5"
  },
  "plugin_start_cmd": "%TP_PLUGIN_FOLDER%TwitchExtras\\nothing.exe",
  "categories": [
    {
      "id": "gitago.twitchextras.main",
      "name": "TE | Predicitions",
      "actions": [
        {
          "id": "gitago.twitchextras.act.prediction.start",
          "name": "Predictions: Set",
          "prefix": "TE | Predicitions",
          "type": "communicate",
          "format": "Start Prediction Title:{$gitago.twitchextras.act.prediction.ptitle$} - Choice #1:{$gitago.twitchextras.act.prediction.p1choice$}    Choice #2{$gitago.twitchextras.act.prediction.p2choice$} for {$gitago.twitchextras.act.prediction.ptime$}seconds",
          "tryInline": true,
          "data": [
            {
              "id": "gitago.twitchextras.act.prediction.ptitle",
              "type": "text",
              "label": "Text",
              "default": "Title"
            },
            {
              "id": "gitago.twitchextras.act.prediction.p1choice",
              "type": "text",
              "label": "Prediction 1 Choice",
              "default": "Enter Prediction 1"
            },
            {
              "id": "gitago.twitchextras.act.prediction.p2choice",
              "type": "text",
              "label": "Prediction 2 Choice",
              "default": "Enter Prediction 2"
            },
            {
              "id": "gitago.twitchextras.act.prediction.ptime",
              "type": "number",
              "label": "Meh",
              "default": "30",
              "allowDecimals": false,
              "minValue": 30,
              "maxValue": 180
            }
          ]
        },
        {
          "id": "gitago.twitchextras.act.prediction.cancelprediction",
          "name": "Predictions: Cancel",
          "prefix": "TE | Predicitions",
          "type": "communicate",
          "format": "Cancel Current Prediction.  ",
          "tryInline": true,
          "data": [
            {
              "id": "gitago.twitchextras.act.prediction.cancel",
              "type": "text",
              "label": "Cancel Prediction",
              "default": "Title"
            }
          ]
        },
        {
          "id": "gitago.twitchextras.act.prediction.pickwinner",
          "name": "Predictions: Select a Winner",
          "prefix": "TE | Predicitions",
          "type": "communicate",
          "format": "Select {$gitago.twitchextras.act.prediction.choice$} as the winner",
          "tryInline": true,
          "data": [
            {
              "id": "gitago.twitchextras.act.prediction.choice",
              "type": "choice",
              "label": "Prediction Choice",
              "default": "Title",
              "valueChoices": [
                "Choice 1",
                "Choice 2"
              ]
            }
          ]
        },
        {
          "id": "gitago.twitchextras.act.poll.start",
          "name": "Poll: Create a Poll",
          "prefix": "TE | Predicitions",
          "type": "communicate",
