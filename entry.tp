          "tryInline": true,
          "data": [
            {
              "id": "gitago.twitchextras.act.poll.ptitle",
              "type": "text",
              "label": "Text",
              "default": "Title"
            },
            {
              "id": "gitago.twitchextras.act.poll.choice1",
              "type": "text",
              "label": "Poll Choice 1",
              "default": "Poll Choice 1"
            },
            {
              "id": "gitago.twitchextras.act.poll.choice2",
              "type": "text",
              "label": "Poll Choice 2",
